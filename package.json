{
  "name": "mute",
  "version": "0.1.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "ng": "ng",
    "prestart": "pm2 gracefulReload process.yml",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "start-sw": "ng build --env=dev && npm run sw && live-server dist/ --port=4300 --host=0.0.0.0 --entry-file=/index.html",
    "prebuild": "sed -i -- \"s/'.*'/'$(git log -n 1 --pretty=format:'%H')'/g\" src/lastcommithash.ts",
    "build": "ng build --prod --build-optimizer -oh none",
    "prebuilddev": "npm run prebuild",
    "builddev": "ng build --prod -oh none --env ovhdev",
    "postbuilddev": "npm run postbuild",
    "postbuild": "sed -i -- \"s/'.*'/''/g\" src/lastcommithash.ts && npm run sw && zopfli dist/*.js dist/*.css dist/*.html & node scripts/brotli_build_compression.js",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "proto": "pbjs -t static-module -w commonjs --no-verify --no-delimited --no-convert -o src/app/doc/network/message_pb.js src/app/doc/network/message.proto && pbts -o src/app/doc/network/message_pb.d.ts src/app/doc/network/message_pb.js && pbjs -t static-module -w commonjs --no-verify --no-delimited --no-convert -o src/app/doc/editor/cursor/cursor_pb.js src/app/doc/editor/cursor/cursor.proto && pbts -o src/app/doc/editor/cursor/cursor_pb.d.ts src/app/doc/editor/cursor/cursor_pb.js",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "cz": "git-cz",
    "sw": "workbox generate:sw",
    "build-desktop": "nativefier --name 'Mute' 'https://www.coedit.re' ./dist-desktop"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
    "Matthieu Nicolas <matthieu.nicolas@inria.fr>",
    "Quentin Tardivon",
    "Camille Menou",
    "Yann Prono"
  ],
  "license": "AGPL-3.0",
  "angular-cli": {},
  "dependencies": {
    "@angular/animations": "^4.4.3",
    "@angular/cdk": "2.0.0-beta.11",
    "@angular/common": "^4.4.3",
    "@angular/compiler": "^4.4.3",
    "@angular/core": "^4.4.3",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@angular/forms": "^4.4.3",
    "@angular/http": "^4.4.3",
    "@angular/material": "2.0.0-beta.11",
    "@angular/platform-browser": "^4.4.3",
    "@angular/platform-browser-dynamic": "^4.4.3",
    "@angular/router": "^4.4.3",
    "@types/text-encoding": "0.0.32",
    "bragi-browser": "0.0.12",
    "codemirror": "~5.28.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "hypermd": "git+https://github.com/coast-team/HyperMD.git",
    "jio": "git+https://github.com/Nexedi/jio.git",
    "loglevel": "^1.4.1",
    "material-design-icons": "^3.0.1",
    "mathjax": "^2.7.1",
    "mnemonicjs": "git+https://github.com/oster/mnemonic.js.git",
    "mute-core": "^0.3.4",
    "mute-structs": "^0.3.3",
    "netflux": "^2.0.0",
    "normalize.css": "^7.0.0",
    "protobufjs": "^6.8.0",
    "rxjs": "^5.4.3",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.3",
    "@angular/compiler-cli": "^4.4.3",
    "@angular/language-service": "^4.4.3",
    "@types/codemirror": "^0.0.46",
    "@types/jasmine": "^2.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/mathjax": "0.0.32",
    "@types/node": "^8.0.31",
    "codelyzer": "~3.1.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "pm2": "^2.7.1",
    "protractor": "~5.1.2",
    "sigver": "^15.0.4",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "~2.3.3",
    "validate-commit-msg": "^2.14.0",
    "workbox-cli": "^2.0.3"
  },
  "repository": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
