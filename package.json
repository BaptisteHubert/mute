{
  "name": "mute",
  "version": "0.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "postbuild": "gzip dist/*.js dist/*.css --keep",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "protoc": "protoc/protoc-3.2.0-linux-x86_64 --js_out=import_style=commonjs,binary:. src/app/doc/network/message.proto src/app/doc/editor/cursor/cursor.proto",
    "pregh-deploy": "npm run build -- --env ghpages --aot --base-href '/mute/'",
    "gh-deploy": "ngh --message \"$(git rev-parse HEAD)\"",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "cz": "git-cz"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
    "Matthieu Nicolas <matthieu.nicolas@inria.fr>"
  ],
  "license": "AGPL-3.0",
  "angular-cli": {},
  "dependencies": {
    "@angular/common": "^2.4.2",
    "@angular/compiler": "^2.4.2",
    "@angular/core": "^2.4.2",
    "@angular/flex-layout": "2.0.0-rc.1",
    "@angular/forms": "^2.4.2",
    "@angular/http": "^2.4.2",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": "^2.4.2",
    "@angular/platform-browser-dynamic": "^2.4.2",
    "@angular/router": "^3.4.2",
    "bragi-browser": "^0.0.12",
    "codemirror": "^5.22",
    "core-js": "^2.4.1",
    "google-protobuf": "^3.2.0",
    "hammerjs": "^2.0.8",
    "jio": "git+https://github.com/Nexedi/jio.git",
    "mnemonicjs": "git+https://github.com/oster/mnemonic.js.git",
    "loglevel": "^1.4.1",
    "material-design-icons": "^3.0.1",
    "mute-core": "0.1.3",
    "mute-structs": "latest",
    "netflux": "latest",
    "normalize.css": "^5.0.0",
    "random-material-color": "^1.0.2",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.2",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "^2.4.2",
    "@types/codemirror": "^0.0.38",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.60",
    "angular-cli-ghpages": "^0.5.0",
    "codelyzer": "~2.0.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.2",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "tslint-eslint-rules": "^3.2.3",
    "typescript": "~2.0.0",
    "validate-commit-msg": "^2.8.2"
  },
  "repository": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
