{
  "name": "mute",
  "version": "0.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "ng": "ng",
    "prestart": "pm2 gracefulReload process.yml",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "start-sw": "npm run build -- --env=dev && npm run sw && cd dist && live-server --port=4300 --host=0.0.0.0 --entry-file=/index.html",
    "prebuild": "sed -i -- \"s/'.*'/'$(git log -n 1 --pretty=format:'%H')'/g\" src/lastcommithash.ts",
    "build": "ng build --prod -oh none",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "proto": "pbjs -t static-module -w commonjs --no-verify --no-delimited --no-convert -o src/app/doc/network/message_pb.js src/app/doc/network/message.proto && pbts -o src/app/doc/network/message_pb.d.ts src/app/doc/network/message_pb.js && pbjs -t static-module -w commonjs --no-verify --no-delimited --no-convert -o src/app/doc/editor/cursor/cursor_pb.js src/app/doc/editor/cursor/cursor.proto && pbts -o src/app/doc/editor/cursor/cursor_pb.d.ts src/app/doc/editor/cursor/cursor_pb.js",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "cz": "git-cz",
    "sw": "workbox-cli generate:sw",
    "firebase-sw-deploy": "npm run build && npm run sw && firebase deploy",
    "build-desktop": "nativefier --name 'Mute' 'https://www.coedit.re' ./dist-desktop"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
    "Matthieu Nicolas <matthieu.nicolas@inria.fr>"
  ],
  "license": "AGPL-3.0",
  "angular-cli": {},
  "dependencies": {
    "@angular/animations": "^4.3.2",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.3.2",
    "@angular/compiler": "^4.3.2",
    "@angular/core": "^4.3.2",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/forms": "^4.3.2",
    "@angular/http": "^4.3.2",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.3.2",
    "@angular/platform-browser-dynamic": "^4.3.2",
    "@angular/router": "^4.3.2",
    "bragi-browser": "0.0.12",
    "codemirror": "^5.27.4",
    "core-js": "^2.4.1",
    "diff": "^3.2.0",
    "google-protobuf": "^3.3.0",
    "hammerjs": "^2.0.8",
    "hypermd": "git+https://github.com/menou2u/HyperMD.git",
    "jio": "git+https://github.com/Nexedi/jio.git",
    "loglevel": "^1.4.1",
    "material-design-icons": "^3.0.1",
    "mathjax": "^2.7.1",
    "mnemonicjs": "git+https://github.com/oster/mnemonic.js.git",
    "mute-core": "^0.2.2",
    "mute-structs": "^0.1.3",
    "netflux": "^1.1.0-beta.7",
    "normalize.css": "^7.0.0",
    "protobufjs": "^6.8.0",
    "rxjs": "^5.4.1",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.6",
    "@angular/compiler-cli": "^4.3.2",
    "@angular/language-service": "^4.3.2",
    "@types/codemirror": "^0.0.42",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.17",
    "codelyzer": "~3.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "pm2": "^2.6.1",
    "protractor": "~5.1.2",
    "sigver": "^15.0.1",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "~2.3.3",
    "validate-commit-msg": "^2.14.0",
    "workbox-cli": "^1.0.1"
  },
  "repository": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
