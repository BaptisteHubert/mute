{
  "name": "mute",
  "version": "0.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build --prod -oh none",
    "postbuild": "zopfli dist/*.js dist/*.css dist/*.html & node scripts/brotli_build_compression.js",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "protoc": "protoc/protoc-3.2.0-linux-x86_64 --js_out=import_style=commonjs,binary:. src/app/doc/network/message.proto src/app/doc/editor/cursor/cursor.proto",
    "pregh-deploy": "npm run build -- --env ghpages --aot --base-href '/mute/'",
    "gh-deploy": "ngh --message \"$(git rev-parse HEAD)\"",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "cz": "git-cz"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
    "Matthieu Nicolas <matthieu.nicolas@inria.fr>"
  ],
  "license": "AGPL-3.0",
  "angular-cli": {},
  "dependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "bragi-browser": "0.0.12",
    "codemirror": "^5.26.0",
    "core-js": "^2.4.1",
    "google-protobuf": "^3.3.0",
    "hammerjs": "^2.0.8",
    "jio": "git+https://github.com/Nexedi/jio.git",
    "loglevel": "^1.4.1",
    "material-design-icons": "^3.0.1",
    "mnemonicjs": "git+https://github.com/oster/mnemonic.js.git",
    "mute-core": "^0.1.7",
    "mute-structs": "^0.1.3",
    "netflux": "^1.1.0-beta.3",
    "normalize.css": "^6.0.0",
    "rxjs": "^5.4.0",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "1.1.1",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/language-service": "^4.0.0",
    "@types/codemirror": "^0.0.38",
    "@types/jasmine": "2.5.45",
    "@types/node": "^6.0.77",
    "angular-cli-ghpages": "^0.5.1",
    "codelyzer": "~3.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.4",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "tslint-eslint-rules": "^3.2.3",
    "typescript": "~2.3.3",
    "validate-commit-msg": "^2.8.2"
  },
  "repository": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
