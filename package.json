    "@angular/compiler": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/material": "2.0.0-alpha.10",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/router": "3.2.3",
    "bragi-browser": "^0.0.12",
    "codemirror": "^5.20.2",
    "core-js": "^2.4.1",
    "loglevel": "^1.4.1",
    "material-design-icons": "^3.0.1",
