{
  "name": "mute",
  "version": "0.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "start-sw": "npm run build -- --env=dev && npm run sw && cd dist && live-server --port=4300 --host=0.0.0.0 --entry-file=/index.html",
    "prebuild": "sed -i -- \"s/'.*'/'$(git log -n 1 --pretty=format:'%H')'/g\" src/lastcommithash.ts",
    "build": "ng build --prod -oh none",
    "postbuild": "sed -i -- \"s/'.*'/''/g\" src/lastcommithash.ts && npm run sw && zopfli dist/*.js dist/*.css dist/*.html & node scripts/brotli_build_compression.js",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "protoc": "protoc/protoc-3.2.0-linux-x86_64 --js_out=import_style=commonjs,binary:. src/app/doc/network/message.proto src/app/doc/editor/cursor/cursor.proto",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "cz": "git-cz",
    "sw": "sw-precache --verbose --config=sw-precache.conf.js",
    "firebase-sw-deploy": "npm run build && firebase deploy",
    "generate-native": "nativefier --name 'Mute' 'https://coedit.re/'"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
    "Matthieu Nicolas <matthieu.nicolas@inria.fr>"
  ],
  "license": "AGPL-3.0",
  "angular-cli": {},
  "dependencies": {
    "@angular/animations": "^4.2.2",
    "@angular/common": "^4.2.2",
    "@angular/compiler": "^4.2.2",
    "@angular/core": "^4.2.2",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "^4.2.2",
    "@angular/http": "^4.2.2",
    "@angular/material": "^2.0.0-beta.7",
    "@angular/platform-browser": "^4.2.2",
    "@angular/platform-browser-dynamic": "^4.2.2",
    "@angular/router": "^4.2.2",
    "bragi-browser": "0.0.12",
    "codemirror": "^5.26.0",
    "core-js": "^2.4.1",
    "google-protobuf": "^3.3.0",
    "hammerjs": "^2.0.8",
    "netflux": "latest",
    "jio": "git+https://github.com/Nexedi/jio.git",
    "mnemonicjs": "git+https://github.com/oster/mnemonic.js.git",
    "loglevel": "^1.4.1",
    "material-design-icons": "^3.0.1",
    "mute-core": "^0.1.7",
    "mute-structs": "^0.1.3",
    "normalize.css": "^6.0.0",
    "rxjs": "^5.4.0",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "1.1.3",
    "@angular/compiler-cli": "^4.2.2",
    "@angular/language-service": "^4.2.2",
    "@types/codemirror": "^0.0.41",
    "@types/jasmine": "2.5.45",
    "@types/node": "^6.0.78",
    "codelyzer": "~3.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.4",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "protractor": "~5.1.2",
    "sw-precache": "^5.2.0",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "~2.3.3",
    "validate-commit-msg": "^2.12.2"
  },
  "repository": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
