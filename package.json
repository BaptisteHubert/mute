{
  "name": "mute",
  "version": "0.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "protoc": "protoc/protoc-3.2.0rc2-linux-x86_64 --js_out=import_style=commonjs,binary:. src/app/doc/network/message.proto src/app/doc/editor/cursor/cursor.proto",
    "gh-deploy": "ng github-pages:deploy --aot --environment ghpages --message \"$(git rev-parse HEAD)\"",
    "commitmsg": "validate-commit-msg",
    "commit": "git cz"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
    "Matthieu Nicolas <matthieu.nicolas@inria.fr>"
  ],
  "license": "AGPL-3.0",
  "angular-cli": {},
  "dependencies": {
    "@angular/common": "^2.4.2",
    "@angular/compiler": "^2.4.2",
    "@angular/core": "^2.4.2",
    "@angular/flex-layout": "^2.0.0-beta.3",
    "@angular/forms": "^2.4.2",
    "@angular/http": "^2.4.2",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "^2.4.2",
    "@angular/platform-browser-dynamic": "^2.4.2",
    "@angular/router": "^3.4.2",
    "bragi-browser": "^0.0.12",
    "codemirror": "^5.22",
    "core-js": "^2.4.1",
    "google-protobuf": "^3.2.0",
    "hammerjs": "^2.0.8",
    "jio": "git+https://github.com/Nexedi/jio.git",
    "loglevel": "^1.4.1",
    "material-design-icons": "^3.0.1",
    "mute-core": "^0.1.1",
    "mute-structs": "latest",
    "netflux": "latest",
    "normalize.css": "^5.0.0",
    "random-material-color": "^1.0.2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.3",
    "ts-helpers": "^1.1.2",
    "tslint-eslint-rules": "^3.2.3",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.30",
    "@angular/compiler-cli": "^2.4.2",
    "@types/codemirror": "^0.0.37",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "husky": "^0.12",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.1",
    "typescript": "~2.0.3",
    "validate-commit-msg": "^2.8.2"
  },
  "repository": {},
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
